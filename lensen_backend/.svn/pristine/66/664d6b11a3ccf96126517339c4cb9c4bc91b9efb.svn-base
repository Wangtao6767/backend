.login-back {
    position: fixed;
    width: 100%;
    height: 100%;
    /*background: url(../../../dest/public/images/back_03.jpg) no-repeat;*/
    background: #D7D7D6;
    background-size: cover
}

.login {
    width: 380px;
    height: 320px;
    background: #ffffff;
    margin: 0 auto;
    border-radius: 8px;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    padding: 20px
}

.login .login-come {
    font-size: 16px;
    color: #fff;
    text-align: center;
    width: 100%;
    height: 30px;
    line-height: 30px
}

.same {
    height: 40px;
    border-radius: 4px;
    border-color: #e8e8e8;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    margin-top: 20px;
    float: left
}

.login .login-password,
.login .login-user {
    width: 98%
}

.login-validation .validation {
    width: 60%;
    float: left
}

.login-validation .validation-chang {
    width: 100px;
    height: 40px;
    margin-top: 20px;
    margin-left: 20px;
    cursor: pointer;
    float: left;
    display: block
}

.login .login-submit {
    border-radius: 20px;
    width: 100%;
    height: 40px;
    background: #252328;
    display: block;
    color: #fff;
    text-align: center;
    line-height: 40px
}

.login .login-submit:hover {
    background: #201f23
}